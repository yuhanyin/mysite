<% provide(:title, "All users") %>
<h1>All users</h1>

<ul class="users">
  <% @users.each do |user| %>
    <li>
      <%= link_to user.name, user %>
      <% if isAdmin %>
       | <%= link_to("delete", user, method: :delete, data: { confirm: "Are you sure?" }) %>
       <% end %>
    </li>
  <% end %>
</ul>
<%= will_paginate %>